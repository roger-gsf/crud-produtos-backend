datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Produto {
  produto_id    Int    @id @default(autoincrement())
  produto_nome  String
  produto_desc  String
  produto_preco Decimal @db.Decimal(5, 2)
}
